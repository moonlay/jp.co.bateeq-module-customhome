<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Directory:etc/zip_codes.xsd">
    <zip countryCode="JP">
        <codes>
            <code id="pattern_1" active="true" example="1234567">^[0-9]{7}$</code>
            <code id="pattern_2" active="true" example="123-4567">^[0-9]{3}\-[0-9]{4}$</code>
        </codes>
    </zip>
</config>
